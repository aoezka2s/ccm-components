{
  "main": {
    "tag": "section",
    "class": "main"
  },
  "comment": {
    "tag": "div",
    "class": "comment",
    "inner": [
      {
        "tag": "p",
        "class": "user",
        "inner": "%user%"
      },
      {
        "tag": "p",
        "class": "date",
        "inner": "%date%"
      },
      {
        "tag": "p",
        "class": "titel",
        "inner": "%titel%"
      },
      {
        "tag": "p",
        "class": "text",
        "inner": "%text%"
      }
    ]
  },
  "input": {
    "tag": "div",
    "class": "form_div",
    "inner": {
      "tag": "form",
      "inner": [
        {
          "tag": "br"
        },
        {
          "tag": "input",
          "placeholder": "Titel",
          "class": "comment_titel"
        },
        {
          "tag": "br"
        },
        {
          "tag": "textarea",
          "placeholder": "Text",
          "class": "comment_text"
        },
        {
          "tag": "br"
        },
        {
          "tag": "input",
          "type": "submit",
          "value": "Submit Comment!"
        }
      ],
      "onsubmit": "%onsubmit%"
    }
  }
}